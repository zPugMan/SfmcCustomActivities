{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Microsoft.AspNetCore.Mvc.Razor.Internal": "Warning",
        "Microsoft.AspNetCore.Mvc.Razor.Razor": "Debug",
        "Microsoft.AspNetCore.Mvc.Razor": "Error",
        "Default": "Information"
      }
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Warning",
        "SfmcCustomActivities": "Trace",
        "SfmcCustomActivities.Controllers.Activities": "Trace"
      }
    }
  },
  "Settings": {
    "SMS": {
      "ConcurrentRequests": 5,
      "JWTEnabled": false
    }
  },
  "AllowedHosts": "*"
  }
